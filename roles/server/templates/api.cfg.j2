[buildout]
extends = production.cfg

[circus.ini]
web_port = {{ proxy_port }}
web_host = {{ proxy_host }}
{% if sandbox_mode is defined %}
sandbox_mode = {{ sandbox_mode }}
{% endif %}

[openprocurement.integrations.edr.ini]
edr_api_host = {{ edr_api_host }}
edr_api_token = {{ edr_api_token }}
edr_api_port = {{ edr_api_port }}
edr_timeout = {{ edr_timeout }}

[logrotate.conf]
logs_bucket = {{ logs_bucket }}
logs_dir = {{ logs_dir }}
aws_access_key = {{ logs_aws_access_key }}
aws_secret_key = {{ logs_aws_secret_key }}
