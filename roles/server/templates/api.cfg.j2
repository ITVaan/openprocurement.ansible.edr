[buildout]
extends = production.cfg

[circus.ini]
web_port = {{ proxy_port }}
web_host = {{ proxy_host }}
{% if sandbox_mode is defined %}
sandbox_mode = {{ sandbox_mode }}
{% endif %}

[openprocurement.integrations.edr.ini]
edr_api_host = {{ edr_api_host }}
edr_api_port = {{ edr_api_port }}
edr_timeout_min = {{ edr_timeout_min }}
edr_timeout_max = {{ edr_timeout_max }}
edr_timeout_step = {{ edr_timeout_step }}
edr_timeout_mode = {{ edr_timeout_mode }}
cache_db_name = {{cache_db_name}}
cache_host = {{cache_host}}
cache_port = {{cache_port}}
time_to_live = {{time_to_live}}
time_to_live_negative = {{time_to_live_negative}}

[logrotate.conf]
logs_bucket = {{ logs_bucket }}
logs_dir = {{ logs_dir }}
aws_access_key = {{ logs_aws_access_key }}
aws_secret_key = {{ logs_aws_secret_key }}
